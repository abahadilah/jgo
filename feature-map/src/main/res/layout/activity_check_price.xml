<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.core.widget.NestedScrollView
        android:background="@drawable/bg_menu"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:padding="@dimen/dimen_16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvVehicleType"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_vehicle_type"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etVehicleType"
                android:layout_marginTop="@dimen/dimen_8dp"
                style="@style/B1"
                android:hint="@string/price_select_vehicle_type"
                app:inputType="popup"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                android:drawablePadding="@dimen/dimen_4dp"
                app:layout_constraintTop_toBottomOf="@id/tvVehicleType"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etVehicleType"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvOriginAddress"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_origin_address"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etOriginAddress"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:layout_constraintTop_toBottomOf="@id/tvOriginAddress"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivOriginAddressMap"
                android:layout_marginEnd="@dimen/dimen_4dp"
                app:inputType="popup"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                android:hint="@string/price_origin_address"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivOriginAddressMap"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etOriginAddress"
                app:layout_constraintBottom_toBottomOf="@id/etOriginAddress"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etOriginAddress"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvDestinationAddress1"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_destination_address_1"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etDestinationAddress1"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivDestinationAddress1Map"
                android:layout_marginEnd="@dimen/dimen_4dp"
                android:hint="@string/price_destination_address_1"
                app:inputType="popup"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivDestinationAddress1Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etDestinationAddress1"
                app:layout_constraintBottom_toBottomOf="@id/etDestinationAddress1"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etDestinationAddress1"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvDestinationAddress2"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_destination_address_2"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etDestinationAddress2"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivDestinationAddress2Map"
                android:layout_marginEnd="@dimen/dimen_4dp"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                android:hint="@string/price_destination_address_2"
                app:inputType="popup"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivDestinationAddress2Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etDestinationAddress2"
                app:layout_constraintBottom_toBottomOf="@id/etDestinationAddress2"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etDestinationAddress2"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvDestinationAddress3"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_destination_address_3"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etDestinationAddress3"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivDestinationAddress3Map"
                android:layout_marginEnd="@dimen/dimen_4dp"
                android:hint="@string/price_destination_address_3"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                app:inputType="popup"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivDestinationAddress3Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etDestinationAddress3"
                app:layout_constraintBottom_toBottomOf="@id/etDestinationAddress3"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etDestinationAddress3"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvDestinationAddress4"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_destination_address_4"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etDestinationAddress4"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivDestinationAddress4Map"
                android:layout_marginEnd="@dimen/dimen_4dp"
                android:hint="@string/price_destination_address_4"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                app:inputType="popup"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivDestinationAddress4Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etDestinationAddress4"
                app:layout_constraintBottom_toBottomOf="@id/etDestinationAddress4"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatTextView
                app:layout_constraintTop_toBottomOf="@id/etDestinationAddress4"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/tvDestinationAddress5"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/price_destination_address_5"
                style="@style/B1"
                android:textColor="@color/colorNeutral50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <id.co.edtslib.edtsds.textfield.TextFieldView
                android:id="@+id/etDestinationAddress5"
                android:layout_marginTop="@dimen/dimen_8dp"
                app:errorTextColor="@color/colorSupportError"
                app:errorTextAppearance="@style/B2"
                app:errorIconDrawable="@null"
                app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivDestinationAddress5Map"
                android:layout_marginEnd="@dimen/dimen_4dp"
                android:hint="@string/price_destination_address_5"
                app:inputType="popup"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivDestinationAddress5Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/etDestinationAddress5"
                app:layout_constraintBottom_toBottomOf="@id/etDestinationAddress5"
                android:src="@drawable/ic_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
    <id.co.edtslib.edtsds.ButtonView
        android:layout_marginTop="@dimen/dimen_1dp"
        android:layout_marginStart="@dimen/dimen_16dp"
        android:layout_marginEnd="@dimen/dimen_16dp"
        android:layout_marginBottom="@dimen/dimen_16dp"
        android:id="@+id/bvSubmit"
        app:variant="primary"
        app:size="medium"
        android:text="@string/price_check"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</androidx.appcompat.widget.LinearLayoutCompat>