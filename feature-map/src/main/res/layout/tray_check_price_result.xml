<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:background="@drawable/bg_menu"
        android:layout_weight="1"
        android:layout_height="0dp">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_marginBottom="@dimen/dimen_2dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.google.android.material.card.MaterialCardView
                android:layout_marginStart="@dimen/dimen_12dp"
                android:layout_marginEnd="@dimen/dimen_12dp"
                android:layout_marginTop="@dimen/dimen_12dp"
                app:cardCornerRadius="@dimen/dimen_8dp"
                app:cardBackgroundColor="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:paddingStart="@dimen/dimen_12dp"
                    android:paddingEnd="@dimen/dimen_12dp"
                    android:paddingTop="@dimen/dimen_16dp"
                    android:paddingBottom="@dimen/dimen_16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:id="@+id/lbVehicleType"
                        style="@style/B1"
                        android:text="@string/price_vehicle_type"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbVehicleType"
                        app:layout_constraintStart_toEndOf="@id/lbVehicleType"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:gravity="end"
                        android:id="@+id/tvVehicleType"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvVehicleType"
                        android:id="@+id/lbOriginAddress"
                        style="@style/B1"
                        android:text="@string/price_origin_address"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbOriginAddress"
                        app:layout_constraintStart_toEndOf="@id/lbOriginAddress"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:id="@+id/tvOriginAddress"
                        android:gravity="end"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvOriginAddress"
                        android:id="@+id/lbDestinationAddress1"
                        style="@style/B1"
                        android:text="@string/price_destination_address_1"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDestinationAddress1"
                        app:layout_constraintStart_toEndOf="@id/lbDestinationAddress1"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:id="@+id/tvDestinationAddress1"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:gravity="end"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress1"
                        android:id="@+id/lbDestinationAddress2"
                        style="@style/B1"
                        android:text="@string/price_destination_address_2"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDestinationAddress2"
                        app:layout_constraintStart_toEndOf="@id/lbDestinationAddress2"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:id="@+id/tvDestinationAddress2"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:gravity="end"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress2"
                        android:id="@+id/lbDestinationAddress3"
                        style="@style/B1"
                        android:text="@string/price_destination_address_3"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDestinationAddress3"
                        app:layout_constraintStart_toEndOf="@id/lbDestinationAddress3"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:gravity="end"
                        android:id="@+id/tvDestinationAddress3"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress3"
                        android:id="@+id/lbDestinationAddress4"
                        style="@style/B1"
                        android:text="@string/price_destination_address_4"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDestinationAddress4"
                        app:layout_constraintStart_toEndOf="@id/lbDestinationAddress4"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:gravity="end"
                        android:id="@+id/tvDestinationAddress4"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDestinationAddress4"
                        android:id="@+id/lbDestinationAddress5"
                        style="@style/B1"
                        android:text="@string/price_destination_address_5"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDestinationAddress5"
                        app:layout_constraintStart_toEndOf="@id/lbDestinationAddress5"
                        android:layout_marginStart="@dimen/dimen_4dp"
                        style="@style/B1"
                        android:gravity="end"
                        android:id="@+id/tvDestinationAddress5"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>
            <androidx.appcompat.widget.AppCompatTextView
                style="@style/H2"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:layout_marginStart="@dimen/dimen_16dp"
                android:textColor="@color/colorNeutral70"
                android:text="@string/price_check_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <com.google.android.material.card.MaterialCardView
                android:layout_marginStart="@dimen/dimen_12dp"
                android:layout_marginEnd="@dimen/dimen_12dp"
                android:layout_marginTop="@dimen/dimen_16dp"
                app:cardCornerRadius="@dimen/dimen_8dp"
                app:cardBackgroundColor="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:paddingStart="@dimen/dimen_12dp"
                    android:paddingEnd="@dimen/dimen_12dp"
                    android:paddingTop="@dimen/dimen_16dp"
                    android:paddingBottom="@dimen/dimen_16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:id="@+id/lbDistance"
                        style="@style/B1"
                        android:text="@string/price_distance"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDistance"
                        app:layout_constraintStart_toEndOf="@id/lbDistance"
                        android:layout_marginStart="@dimen/dimen_8dp"
                        style="@style/B1"
                        android:gravity="end"
                        android:id="@+id/tvDistance"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDistance"
                        android:id="@+id/lbDuration"
                        style="@style/B1"
                        android:text="@string/price_duration"
                        android:visibility="gone"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbDuration"
                        style="@style/B1"
                        android:visibility="gone"
                        android:id="@+id/tvDuration"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_marginTop="@dimen/dimen_12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvDuration"
                        android:id="@+id/lbPrice"
                        style="@style/H2"
                        android:text="@string/price_cost"
                        android:textColor="@color/colorNeutral50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <androidx.appcompat.widget.AppCompatTextView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/lbPrice"
                        style="@style/H2"
                        android:id="@+id/tvPrice"
                        android:textColor="@color/colorNeutral70"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>
            <id.co.edtslib.edtsds.ButtonView
                android:layout_marginStart="@dimen/dimen_16dp"
                android:layout_marginEnd="@dimen/dimen_16dp"
                android:id="@+id/bvSubmit"
                android:text="@string/finish"
                android:textAllCaps="true"
                android:layout_marginTop="@dimen/dimen_24dp"
                app:layout_constraintTop_toBottomOf="@id/tvPrice"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</androidx.appcompat.widget.LinearLayoutCompat>